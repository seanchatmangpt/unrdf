{
  "name": "unrdf-server-example",
  "version": "1.0.0",
  "description": "Node.js backend demonstrating UNRDF server-side packages",
  "type": "module",
  "main": "src/index.mjs",
  "scripts": {
    "dev": "node --watch src/index.mjs",
    "start": "node src/index.mjs",
    "test": "vitest run",
    "clean": "rm -rf node_modules"
  },
  "dependencies": {
    "@unrdf/core": "workspace:*",
    "@unrdf/hooks": "workspace:*",
    "@unrdf/federation": "workspace:*",
    "@unrdf/streaming": "workspace:*",
    "@unrdf/cli": "workspace:*",
    "@unrdf/knowledge-engine": "workspace:*",
    "@unrdf/dark-matter": "workspace:*",
    "ws": "^8.18.3",
    "n3": "^1.26.0",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "vitest": "^1.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
