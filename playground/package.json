{
  "name": "unrdf-playground",
  "version": "2.0.0",
  "description": "Citty + Nunjucks + UNRDF playground CLI demonstrating autonomic knowledge graph systems",
  "type": "module",
  "private": true,
  "bin": {
    "playground": "./src/cli/index.mjs"
  },
  "scripts": {
    "dev": "node --watch examples/basic-usage.mjs",
    "cli": "node src/cli/index.mjs",
    "cli:dev": "node --watch src/cli/index.mjs",
    "examples": "node examples/basic-usage.mjs",
    "sparql": "node examples/sparql-queries.mjs",
    "validation": "node examples/validation.mjs",
    "reasoning": "node examples/reasoning.mjs",
    "turtle": "node examples/turtle-parsing.mjs",
    "zod": "node examples/zod-integration.mjs",
    "cli-test": "node examples/cli-testing.mjs",
    "cli-scenarios": "node examples/cli-scenarios.mjs",
    "hooks": "node examples/hooks-runtime.mjs",
    "server": "nitro dev",
    "nitro:dev": "nitro dev",
    "nitro:build": "nitro build",
    "nitro:preview": "nitro preview",
    "test": "vitest run --config vitest.config.mjs",
    "test:watch": "vitest --config vitest.config.mjs --watch",
    "test:ui": "vitest --config vitest.config.mjs --ui",
    "test:integration": "vitest run --config test/integration.config.mjs",
    "test:integration:watch": "vitest --config test/integration.config.mjs --watch",
    "test:integration:coverage": "vitest run --config test/integration.config.mjs --coverage",
    "test:integration:ui": "vitest --config test/integration.config.mjs --ui",
    "lint": "eslint src/ --ext .mjs",
    "typecheck": "tsc --noEmit --allowJs src/",
    "gen:completions": "node scripts/generate-completions.mjs",
    "gen:ontology": "node scripts/generate-ontology.mjs"
  },
  "dependencies": {
    "bcrypt": "^6.0.0",
    "chalk": "^5.3.0",
    "citty": "^0.1.6",
    "citty-test-utils": "^0.5.1",
    "cors": "^2.8.5",
    "esbuild": "^0.20.0",
    "express": "^5.1.0",
    "express-rate-limit": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "nitro": "^2.2.28",
    "nunjucks": "^3.2.4",
    "table": "^6.8.1",
    "unrdf": "file:../",
    "vitest": "^3.2.4",
    "ws": "^8.18.3",
    "yaml": "^2.3.4",
    "yargs-parser": "^21.1.1",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@vitest/ui": "^3.2.4",
    "eslint": "^8.56.0",
    "mocha": "^10.2.0",
    "chai": "^4.3.10"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "cli",
    "rdf",
    "knowledge-graph",
    "sparql",
    "nunjucks",
    "citty"
  ],
  "packageManager": "pnpm@9.15.1"
}
