@prefix ex: <http://example.org/> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix schema: <http://schema.org/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

# Person data
ex:person1 a schema:Person ;
    schema:name "Alice Johnson" ;
    schema:age "30"^^xsd:integer ;
    schema:email "alice@example.org" ;
    schema:jobTitle "Software Engineer" .

ex:person2 a schema:Person ;
    schema:name "Bob Smith" ;
    schema:age "35"^^xsd:integer ;
    schema:email "bob@example.org" ;
    schema:jobTitle "Data Scientist" .

ex:person3 a schema:Person ;
    schema:name "Carol Davis" ;
    schema:age "28"^^xsd:integer ;
    schema:email "carol@example.org" ;
    schema:jobTitle "Product Manager" .

# Organization data
ex:org1 a schema:Organization ;
    schema:name "Tech Corp" ;
    schema:url "https://techcorp.example" ;
    schema:numberOfEmployees "1000"^^xsd:integer .

# Relationships
ex:person1 schema:worksFor ex:org1 .
ex:person2 schema:worksFor ex:org1 .
ex:person3 schema:worksFor ex:org1 .

ex:person1 schema:knows ex:person2 .
ex:person2 schema:knows ex:person3 .

# Project data
ex:project1 a schema:Project ;
    schema:name "KGC Sidecar" ;
    schema:description "Knowledge Graph Coordinator Sidecar" ;
    schema:startDate "2025-01-01"^^xsd:date .

ex:person1 schema:contributor ex:project1 .
ex:person2 schema:contributor ex:project1 .
