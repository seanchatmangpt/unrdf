@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix ex: <http://example.org/> .

ex:Person rdf:type owl:Class ;
  rdfs:label {{ "Person" | rdfLiteral("en") }} ;
  rdfs:comment {{ "A human person" | rdfLiteral("en") }} .

ex:hasName rdf:type owl:DatatypeProperty ;
  rdfs:domain ex:Person ;
  rdfs:range {{ "xsd:string" | expand }} .

ex:hasAge rdf:type owl:DatatypeProperty ;
  rdfs:domain ex:Person ;
  rdfs:range {{ "xsd:integer" | expand }} .
