{
  "name": "unrdf-governance-project",
  "version": "1.0.0",
  "description": "UNRDF governance project with policy packs and compliance monitoring",
  "type": "module",
  "main": "src/index.mjs",
  "scripts": {
    "dev": "node src/index.mjs",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:governance": "vitest run test/governance/",
    "lint": "eslint src/ test/ --ext .mjs",
    "format": "prettier --write src/ test/",
    "validate:policies": "node scripts/validate-policies.mjs",
    "audit": "node src/governance/audit-runner.mjs",
    "compliance:report": "node src/governance/compliance-report.mjs"
  },
  "keywords": [
    "unrdf",
    "governance",
    "compliance",
    "policy-pack",
    "audit"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "unrdf": "^2.1.0"
  },
  "devDependencies": {
    "vitest": "^1.0.0",
    "eslint": "^8.0.0",
    "prettier": "^3.0.0"
  }
}
