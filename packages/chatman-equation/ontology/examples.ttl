ex:MarketAnalyst a prov:Agent ;
    rdfs:label "Market Research Analyst" ;
    rdfs:comment "Agent specializing in market dynamics and competitive analysis" .

ex:StrategyConsultant a prov:Agent ;
    rdfs:label "Strategy Consultant" ;
    rdfs:comment "Agent specializing in blue ocean strategy and innovation" .

ex:FinancialAnalyst a prov:Agent ;
    rdfs:label "Financial Analyst" ;
    rdfs:comment "Agent specializing in financial metrics and valuation" .

ex:TechAnalyst a prov:Agent ;
    rdfs:label "Technology Analyst" ;
    rdfs:comment "Agent specializing in technology market analysis" .

ex:BusinessAnalyst a prov:Agent ;
    rdfs:label "Business Analyst" ;
    rdfs:comment "Agent specializing in business model and operational analysis" .

# End of Examples
