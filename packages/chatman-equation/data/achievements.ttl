    dcterms:creator chatman:sean-chatman ;
    chatman:category "Theoretical Framework"^^xsd:string ;
    chatman:field "Knowledge Graph Theory"^^xsd:string ;
    chatman:primaryConstant "Θ = 8"^^xsd:string ;
    chatman:fullExpression """Under the L1-cache cost model with branchless compilation:
All hook evaluations complete in ≤ 8 primitive operations

Breakdown:
  1. Cache fetch (predicate)        [1 op]
  2. Cache fetch (binding)          [1 op]
  3. Comparison/arithmetic          [1 op]
  4. Boolean combine                [1 op]
  5. Effect dispatch table lookup   [2 ops]
  6. Delta application              [1 op]
  7. Receipt hash update            [1 op]
"""^^xsd:string ;
    chatman:performance "Sub-microsecond hook execution (0.017ms P95 measured)"^^xsd:string ;
    chatman:scalability "365 million operations per second sustained"^^xsd:string ;
    chatman:determinism "Cryptographic proof of every operation"^^xsd:string ;
    chatman:temporalGuarantees "4D time-travel with universe freezing"^^xsd:string .

