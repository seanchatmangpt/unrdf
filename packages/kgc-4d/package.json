{
  "name": "@unrdf/kgc-4d",
  "version": "5.0.1",
  "description": "KGC 4D Datum & Universe Freeze Engine - Nanosecond-precision event logging with Git-backed snapshots",
  "type": "module",
  "exports": {
    ".": "./src/index.mjs",
    "./client": "./src/client.mjs",
    "./hdit": "./src/hdit/index.mjs"
  },
  "main": "./src/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "src",
    "docs",
    "examples"
  ],
  "scripts": {
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:doctest": "node scripts/generate-doctests.mjs && vitest run test/doctest/",
    "test:doctest:watch": "vitest watch test/doctest/",
    "test:all": "npm run test:doctest && npm test",
    "pretest": "node scripts/generate-doctests.mjs",
    "benchmark": "node test/benchmarks/run-benchmarks.mjs",
    "build": "unbuild && tsc --emitDeclarationOnly || true",
    "build:types": "tsc --emitDeclarationOnly"
  },
  "dependencies": {
    "@unrdf/core": "workspace:*",
    "@unrdf/oxigraph": "workspace:*",
    "hash-wasm": "^4.12.0",
    "isomorphic-git": "^1.35.1"
  },
  "devDependencies": {
    "comment-parser": "^1.4.1",
    "simple-statistics": "^7.8.8",
    "tinybench": "^6.0.0",
    "vitest": "^4.0.15"
  },
  "keywords": [
    "rdf",
    "knowledge",
    "4d",
    "event-sourcing",
    "time-travel",
    "git",
    "nanosecond"
  ],
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=7.0.0"
  },
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/unrdf/unrdf.git",
    "directory": "packages/kgc-4d"
  },
  "bugs": {
    "url": "https://github.com/unrdf/unrdf/issues"
  },
  "homepage": "https://github.com/unrdf/unrdf#readme",
  "author": "UNRDF Contributors",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  }
}
