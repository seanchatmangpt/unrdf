{
  "name": "@unrdf/cli",
  "version": "6.0.0-rc.2",
  "description": "UNRDF CLI - Command-line Tools for Graph Operations and Context Management",
  "type": "module",
  "bin": {
    "unrdf": "./src/cli.mjs"
  },
  "main": "src/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": "./src/index.mjs",
    "./commands": "./src/commands/index.mjs"
  },
  "sideEffects": false,
  "files": [
    "src/",
    "dist/",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "test": "vitest run",
    "test:fast": "vitest run",
    "test:watch": "vitest",
    "build": "unbuild && tsc --emitDeclarationOnly",
    "build:types": "tsc --emitDeclarationOnly",
    "lint": "eslint src/ test/ --max-warnings=0",
    "lint:fix": "eslint src/ test/ --fix",
    "format": "prettier --write src/ test/",
    "format:check": "prettier --check src/ test/",
    "clean": "rm -rf dist/ .nyc_output/ coverage/",
    "dev": "unbuild --watch"
  },
  "keywords": [
    "rdf",
    "cli",
    "command-line",
    "graph",
    "sparql"
  ],
  "dependencies": {
    "@unrdf/core": "workspace:*",
    "@unrdf/decision-fabric": "workspace:*",
    "@unrdf/federation": "workspace:*",
    "@unrdf/hooks": "workspace:*",
    "@unrdf/streaming": "workspace:*",
    "citty": "^0.1.6",
    "table": "^6.9.0",
    "yaml": "^2.8.2"
  },
  "devDependencies": {
    "@types/node": "^25.0.3",
    "citty-test-utils": "^0.1.0",
    "vitest": "^4.0.16"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/unrdf/unrdf.git",
    "directory": "packages/cli"
  },
  "bugs": {
    "url": "https://github.com/unrdf/unrdf/issues"
  },
  "homepage": "https://github.com/unrdf/unrdf#readme",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  }
}
