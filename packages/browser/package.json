{
  "name": "@unrdf/browser",
  "version": "5.0.0-beta.1",
  "description": "UNRDF Browser SDK - Client-side RDF with IndexedDB Storage",
  "type": "module",
  "main": "src/index.mjs",
  "exports": {
    ".": "./src/index.mjs",
    "./store": "./src/store.mjs",
    "./shims": "./src/shims.mjs"
  },
  "sideEffects": false,
  "files": [
    "src/",
    "dist/",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "test": "vitest run --coverage",
    "test:fast": "vitest run --coverage",
    "test:watch": "vitest --coverage",
    "build": "node build.config.mjs",
    "lint": "eslint src/ test/ --max-warnings=0",
    "lint:fix": "eslint src/ test/ --fix",
    "format": "prettier --write src/ test/",
    "format:check": "prettier --check src/ test/",
    "clean": "rm -rf dist/ .nyc_output/ coverage/",
    "dev": "echo 'Development mode for @unrdf/browser'"
  },
  "keywords": [
    "rdf",
    "browser",
    "indexeddb",
    "storage",
    "client-side"
  ],
  "dependencies": {
    "@unrdf/core": "workspace:*",
    "@unrdf/streaming": "workspace:*",
    "n3": "^1.26.0",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@types/node": "^24.10.1",
    "happy-dom": "^20.0.11",
    "jsdom": "^27.2.0",
    "vitest": "^4.0.15"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/unrdf/unrdf.git",
    "directory": "packages/browser"
  },
  "bugs": {
    "url": "https://github.com/unrdf/unrdf/issues"
  },
  "homepage": "https://github.com/unrdf/unrdf#readme",
  "license": "MIT"
}
