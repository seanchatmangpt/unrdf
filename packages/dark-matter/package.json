{
  "name": "@unrdf/dark-matter",
  "version": "5.0.0-beta.1",
  "description": "UNRDF Dark Matter - Query Optimization and Performance Analysis (Optional Extension)",
  "type": "module",
  "main": "src/index.mjs",
  "exports": {
    ".": "./src/index.mjs",
    "./optimizer": "./src/optimizer.mjs",
    "./analyzer": "./src/analyzer.mjs"
  },
  "sideEffects": false,
  "files": [
    "src/",
    "dist/",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "test": "vitest run --coverage",
    "test:fast": "vitest run --coverage",
    "test:watch": "vitest --coverage",
    "build": "node build.config.mjs",
    "lint": "eslint src/ test/ --max-warnings=0",
    "lint:fix": "eslint src/ test/ --fix",
    "format": "prettier --write src/ test/",
    "format:check": "prettier --check src/ test/",
    "clean": "rm -rf dist/ .nyc_output/ coverage/",
    "dev": "echo 'Development mode for @unrdf/dark-matter'"
  },
  "keywords": [
    "rdf",
    "optimization",
    "performance",
    "sparql",
    "80-20"
  ],
  "dependencies": {
    "@unrdf/core": "workspace:*",
    "typhonjs-escomplex": "^0.1.0"
  },
  "devDependencies": {
    "@types/node": "^24.10.1",
    "vitest": "^4.0.15"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/unrdf/unrdf.git",
    "directory": "packages/dark-matter"
  },
  "bugs": {
    "url": "https://github.com/unrdf/unrdf/issues"
  },
  "homepage": "https://github.com/unrdf/unrdf#readme",
  "license": "MIT"
}
