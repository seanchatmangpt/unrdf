{
  "name": "@unrdf/test-utils",
  "version": "6.0.0-rc.2",
  "description": "Testing utilities for UNRDF development",
  "type": "module",
  "main": "./src/index.mjs",
  "exports": {
    ".": "./src/index.mjs"
  },
  "files": [
    "src"
  ],
  "scripts": {
    "test": "vitest run",
    "test:fast": "vitest run --no-coverage",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "keywords": [
    "rdf",
    "testing",
    "utilities"
  ],
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=7.0.0"
  },
  "sideEffects": false,
  "author": "UNRDF Contributors",
  "license": "MIT",
  "private": true,
  "dependencies": {
    "@unrdf/oxigraph": "workspace:*",
    "@unrdf/knowledge-engine": "workspace:*",
    "@opentelemetry/api": "^1.9.0",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "vitest": "^4.0.15"
  }
}
