{
  "dashboard": {
    "title": "YAWL Workflow Observability",
    "description": "Comprehensive dashboard for YAWL workflow engine monitoring with SLIs, task metrics, and distributed tracing correlation",
    "tags": ["yawl", "workflow", "observability", "sli"],
    "timezone": "browser",
    "schemaVersion": 38,
    "version": 1,
    "refresh": "10s",
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "panels": [
      {
        "id": 1,
        "title": "SLO Compliance Score",
        "type": "gauge",
        "gridPos": { "x": 0, "y": 0, "w": 6, "h": 6 },
        "targets": [
          {
            "expr": "yawl_slo_compliance",
            "legendFormat": "Compliance",
            "refId": "A"
          }
        ],
        "options": {
          "orientation": "auto",
          "showThresholdLabels": false,
          "showThresholdMarkers": true
        },
        "fieldConfig": {
          "defaults": {
            "min": 0,
            "max": 1,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                { "value": 0, "color": "red" },
                { "value": 0.8, "color": "yellow" },
                { "value": 0.95, "color": "green" }
              ]
            },
            "unit": "percentunit"
          }
        }
      },
      {
        "id": 2,
        "title": "Workflow Completion Rate",
        "type": "stat",
        "gridPos": { "x": 6, "y": 0, "w": 6, "h": 3 },
        "targets": [
          {
            "expr": "yawl_sli_completion_rate",
            "legendFormat": "Completion Rate",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percentunit",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                { "value": 0, "color": "red" },
                { "value": 0.9, "color": "yellow" },
                { "value": 0.95, "color": "green" }
              ]
            }
          }
        }
      },
      {
        "id": 3,
        "title": "Task Success Rate",
        "type": "stat",
        "gridPos": { "x": 12, "y": 0, "w": 6, "h": 3 },
        "targets": [
          {
            "expr": "yawl_sli_task_success_rate",
            "legendFormat": "Success Rate",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percentunit",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                { "value": 0, "color": "red" },
                { "value": 0.95, "color": "yellow" },
                { "value": 0.99, "color": "green" }
              ]
            }
          }
        }
      },
      {
        "id": 4,
        "title": "P95 Task Latency",
        "type": "stat",
        "gridPos": { "x": 18, "y": 0, "w": 6, "h": 3 },
        "targets": [
          {
            "expr": "yawl_sli_p95_latency_seconds",
            "legendFormat": "P95 Latency",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "s",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                { "value": 0, "color": "green" },
                { "value": 5, "color": "yellow" },
                { "value": 10, "color": "red" }
              ]
            }
          }
        }
      },
      {
        "id": 5,
        "title": "Task Error Rate",
        "type": "stat",
        "gridPos": { "x": 6, "y": 3, "w": 6, "h": 3 },
        "targets": [
          {
            "expr": "yawl_sli_task_error_rate",
            "legendFormat": "Error Rate",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percentunit",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                { "value": 0, "color": "green" },
                { "value": 0.01, "color": "yellow" },
                { "value": 0.05, "color": "red" }
              ]
            }
          }
        }
      },
      {
        "id": 6,
        "title": "Resource Utilization",
        "type": "stat",
        "gridPos": { "x": 12, "y": 3, "w": 6, "h": 3 },
        "targets": [
          {
            "expr": "yawl_sli_resource_utilization",
            "legendFormat": "Utilization",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percentunit",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                { "value": 0, "color": "blue" },
                { "value": 0.7, "color": "green" },
                { "value": 0.9, "color": "red" }
              ]
            }
          }
        }
      },
      {
        "id": 7,
        "title": "Cases by Status",
        "type": "timeseries",
        "gridPos": { "x": 0, "y": 6, "w": 12, "h": 8 },
        "targets": [
          {
            "expr": "rate(yawl_workflow_cases_total{status=\"created\"}[5m])",
            "legendFormat": "Created - {{workflow_id}}",
            "refId": "A"
          },
          {
            "expr": "rate(yawl_workflow_cases_total{status=\"completed\"}[5m])",
            "legendFormat": "Completed - {{workflow_id}}",
            "refId": "B"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "reqps"
          }
        }
      },
      {
        "id": 8,
        "title": "Task Duration Distribution",
        "type": "timeseries",
        "gridPos": { "x": 12, "y": 6, "w": 12, "h": 8 },
        "targets": [
          {
            "expr": "histogram_quantile(0.50, rate(yawl_task_duration_seconds_bucket[5m]))",
            "legendFormat": "p50 - {{task_id}}",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.95, rate(yawl_task_duration_seconds_bucket[5m]))",
            "legendFormat": "p95 - {{task_id}}",
            "refId": "B"
          },
          {
            "expr": "histogram_quantile(0.99, rate(yawl_task_duration_seconds_bucket[5m]))",
            "legendFormat": "p99 - {{task_id}}",
            "refId": "C"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "s"
          }
        }
      },
      {
        "id": 9,
        "title": "Active vs Enabled Tasks",
        "type": "timeseries",
        "gridPos": { "x": 0, "y": 14, "w": 12, "h": 8 },
        "targets": [
          {
            "expr": "yawl_workflow_active_tasks",
            "legendFormat": "Active - {{workflow_id}}",
            "refId": "A"
          },
          {
            "expr": "yawl_workflow_enabled_tasks",
            "legendFormat": "Enabled - {{workflow_id}}",
            "refId": "B"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short"
          }
        }
      },
      {
        "id": 10,
        "title": "Pattern Usage (Split/Join Types)",
        "type": "piechart",
        "gridPos": { "x": 12, "y": 14, "w": 6, "h": 8 },
        "targets": [
          {
            "expr": "sum by (pattern_type, operation) (yawl_pattern_usage_count)",
            "legendFormat": "{{pattern_type}} ({{operation}})",
            "refId": "A"
          }
        ],
        "options": {
          "legend": {
            "displayMode": "table",
            "placement": "right"
          }
        }
      },
      {
        "id": 11,
        "title": "Task Errors by Type",
        "type": "bargauge",
        "gridPos": { "x": 18, "y": 14, "w": 6, "h": 8 },
        "targets": [
          {
            "expr": "sum by (error_type) (rate(yawl_task_errors_total[5m]))",
            "legendFormat": "{{error_type}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "reqps"
          }
        }
      },
      {
        "id": 12,
        "title": "Case Completion Time Distribution",
        "type": "heatmap",
        "gridPos": { "x": 0, "y": 22, "w": 12, "h": 8 },
        "targets": [
          {
            "expr": "rate(yawl_case_completion_time_seconds_bucket[5m])",
            "legendFormat": "{{le}}",
            "refId": "A",
            "format": "heatmap"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "s"
          }
        }
      },
      {
        "id": 13,
        "title": "Task Wait Time (Enabled to Started)",
        "type": "timeseries",
        "gridPos": { "x": 12, "y": 22, "w": 12, "h": 8 },
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(yawl_task_wait_time_seconds_bucket[5m]))",
            "legendFormat": "p95 - {{task_id}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "s"
          }
        }
      },
      {
        "id": 14,
        "title": "Resource Allocations by Role",
        "type": "timeseries",
        "gridPos": { "x": 0, "y": 30, "w": 12, "h": 8 },
        "targets": [
          {
            "expr": "rate(yawl_resource_allocations_total[5m])",
            "legendFormat": "{{role}} - {{resource_id}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "reqps"
          }
        }
      },
      {
        "id": 15,
        "title": "Per-Resource Utilization",
        "type": "bargauge",
        "gridPos": { "x": 12, "y": 30, "w": 12, "h": 8 },
        "targets": [
          {
            "expr": "yawl_resource_utilization",
            "legendFormat": "{{resource_id}} ({{role}})",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percentunit",
            "min": 0,
            "max": 1
          }
        }
      }
    ],
    "templating": {
      "list": [
        {
          "name": "workflow_id",
          "label": "Workflow",
          "type": "query",
          "datasource": "Prometheus",
          "query": "label_values(yawl_workflow_cases_total, workflow_id)",
          "multi": true,
          "includeAll": true
        }
      ]
    },
    "annotations": {
      "list": [
        {
          "name": "Case Completed",
          "datasource": "Prometheus",
          "expr": "changes(yawl_workflow_cases_total{status=\"completed\"}[1m]) > 0",
          "iconColor": "green"
        }
      ]
    }
  },
  "overwrite": true
}
