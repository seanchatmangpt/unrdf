    rdfs:range slo:Threshold .

slo:thresholdValue a rdf:Property, owl:DatatypeProperty ;
    rdfs:label "threshold value"@en ;
    rdfs:comment "The numeric value of the threshold."@en ;
    rdfs:domain slo:Threshold ;
    rdfs:range xsd:decimal .

slo:thresholdUnit a rdf:Property, owl:DatatypeProperty ;
    rdfs:label "threshold unit"@en ;
    rdfs:comment "The unit of measurement (ms, percent, requests/sec)."@en ;
