# =============================================================================
# REASONING VOCABULARY
# Enables inference via N3 including logical formulas, proof generation,
# contradiction detection, and optimal path finding
# =============================================================================

# -----------------------------------------------------------------------------
# Core Reasoning Classes
# -----------------------------------------------------------------------------

rsn:Rule a rdfs:Class, owl:Class ;
    rdfs:label "Rule"@en ;
    rdfs:comment "An inference rule for deriving new knowledge."@en .

rsn:Formula a rdfs:Class, owl:Class ;
    rdfs:label "Formula"@en ;
    rdfs:comment "A logical formula in N3 notation."@en .

rsn:Proof a rdfs:Class, owl:Class ;
    rdfs:label "Proof"@en ;
    rdfs:comment "A proof of a conclusion derived from rules and facts."@en .

rsn:ProofStep a rdfs:Class, owl:Class ;
    rdfs:label "Proof Step"@en ;
    rdfs:comment "A single step in a proof derivation."@en .

rsn:Contradiction a rdfs:Class, owl:Class ;
    rdfs:label "Contradiction"@en ;
    rdfs:comment "A detected contradiction in the knowledge base."@en .

rsn:OptimalPath a rdfs:Class, owl:Class ;
    rdfs:label "Optimal Path"@en ;
    rdfs:comment "An optimal path through a state space or graph."@en .

rsn:Inference a rdfs:Class, owl:Class ;
    rdfs:label "Inference"@en ;
    rdfs:comment "An inference derived from rules."@en .

rsn:Axiom a rdfs:Class, owl:Class ;
    rdfs:label "Axiom"@en ;
    rdfs:comment "A foundational axiom that requires no proof."@en .

rsn:Hypothesis a rdfs:Class, owl:Class ;
    rdfs:label "Hypothesis"@en ;
    rdfs:comment "A hypothesis to be proven or refuted."@en .

# -----------------------------------------------------------------------------
# Reasoning Properties
# -----------------------------------------------------------------------------

rsn:hasAntecedent a rdf:Property, owl:ObjectProperty ;
    rdfs:label "has antecedent"@en ;
    rdfs:comment "The premise/antecedent of a rule (the 'if' part)."@en ;
    rdfs:domain rsn:Rule ;
    rdfs:range rsn:Formula .

rsn:hasConsequent a rdf:Property, owl:ObjectProperty ;
    rdfs:label "has consequent"@en ;
    rdfs:comment "The conclusion/consequent of a rule (the 'then' part)."@en ;
    rdfs:domain rsn:Rule ;
    rdfs:range rsn:Formula .

rsn:n3Formula a rdf:Property, owl:DatatypeProperty ;
    rdfs:label "N3 formula"@en ;
    rdfs:comment "The N3 notation of a formula."@en ;
    rdfs:domain rsn:Formula ;
    rdfs:range xsd:string .

rsn:sparqlQuery a rdf:Property, owl:DatatypeProperty ;
    rdfs:label "SPARQL query"@en ;
    rdfs:comment "A SPARQL query equivalent to the formula."@en ;
    rdfs:domain rsn:Formula ;
    rdfs:range xsd:string .

rsn:hasProofStep a rdf:Property, owl:ObjectProperty ;
    rdfs:label "has proof step"@en ;
    rdfs:comment "Links a proof to its steps."@en ;
    rdfs:domain rsn:Proof ;
    rdfs:range rsn:ProofStep .

rsn:stepOrder a rdf:Property, owl:DatatypeProperty ;
    rdfs:label "step order"@en ;
    rdfs:comment "The order of a step in a proof."@en ;
    rdfs:domain rsn:ProofStep ;
    rdfs:range xsd:integer .

rsn:appliedRule a rdf:Property, owl:ObjectProperty ;
    rdfs:label "applied rule"@en ;
    rdfs:comment "The rule applied in a proof step."@en ;
    rdfs:domain rsn:ProofStep ;
    rdfs:range rsn:Rule .

rsn:inputFormula a rdf:Property, owl:ObjectProperty ;
    rdfs:label "input formula"@en ;
    rdfs:comment "Input formulas used in a proof step."@en ;
    rdfs:domain rsn:ProofStep ;
    rdfs:range rsn:Formula .

rsn:outputFormula a rdf:Property, owl:ObjectProperty ;
    rdfs:label "output formula"@en ;
    rdfs:comment "Output formula derived in a proof step."@en ;
    rdfs:domain rsn:ProofStep ;
    rdfs:range rsn:Formula .

rsn:proves a rdf:Property, owl:ObjectProperty ;
    rdfs:label "proves"@en ;
    rdfs:comment "Links a proof to what it proves."@en ;
    rdfs:domain rsn:Proof ;
    rdfs:range rsn:Formula .

rsn:contradicts a rdf:Property, owl:ObjectProperty ;
    rdfs:label "contradicts"@en ;
    rdfs:comment "Links a contradiction to the conflicting formulas."@en ;
    rdfs:domain rsn:Contradiction ;
    rdfs:range rsn:Formula .

rsn:contradictionExplanation a rdf:Property, owl:DatatypeProperty ;
    rdfs:label "contradiction explanation"@en ;
    rdfs:comment "Human-readable explanation of the contradiction."@en ;
    rdfs:domain rsn:Contradiction ;
    rdfs:range xsd:string .

rsn:resolutionStrategy a rdf:Property, owl:DatatypeProperty ;
    rdfs:label "resolution strategy"@en ;
    rdfs:comment "Strategy to resolve the contradiction."@en ;
    rdfs:domain rsn:Contradiction ;
    rdfs:range xsd:string .

rsn:pathFrom a rdf:Property, owl:ObjectProperty ;
    rdfs:label "path from"@en ;
    rdfs:comment "Starting node of an optimal path."@en ;
    rdfs:domain rsn:OptimalPath ;
    rdfs:range rdfs:Resource .

rsn:pathTo a rdf:Property, owl:ObjectProperty ;
    rdfs:label "path to"@en ;
    rdfs:comment "Ending node of an optimal path."@en ;
    rdfs:domain rsn:OptimalPath ;
    rdfs:range rdfs:Resource .

rsn:pathNodes a rdf:Property, owl:ObjectProperty ;
    rdfs:label "path nodes"@en ;
    rdfs:comment "Ordered list of nodes in the path."@en ;
    rdfs:domain rsn:OptimalPath ;
    rdfs:range rdf:List .

rsn:pathCost a rdf:Property, owl:DatatypeProperty ;
    rdfs:label "path cost"@en ;
    rdfs:comment "Total cost of the optimal path."@en ;
    rdfs:domain rsn:OptimalPath ;
    rdfs:range xsd:decimal .

rsn:pathLength a rdf:Property, owl:DatatypeProperty ;
    rdfs:label "path length"@en ;
    rdfs:comment "Number of edges in the path."@en ;
    rdfs:domain rsn:OptimalPath ;
    rdfs:range xsd:integer .

rsn:optimizationCriterion a rdf:Property, owl:DatatypeProperty ;
    rdfs:label "optimization criterion"@en ;
    rdfs:comment "Criterion used for optimization: 'cost', 'time', 'hops'."@en ;
    rdfs:domain rsn:OptimalPath ;
    rdfs:range xsd:string .

rsn:derivedFrom a rdf:Property, owl:ObjectProperty ;
    rdfs:label "derived from"@en ;
    rdfs:comment "Links an inference to its source facts/rules."@en ;
    rdfs:domain rsn:Inference ;
    rdfs:range rdfs:Resource .

rsn:confidence a rdf:Property, owl:DatatypeProperty ;
    rdfs:label "confidence"@en ;
    rdfs:comment "Confidence level of an inference (0-1)."@en ;
    rdfs:domain rsn:Inference ;
    rdfs:range xsd:decimal .

rsn:inferenceStrategy a rdf:Property, owl:DatatypeProperty ;
    rdfs:label "inference strategy"@en ;
    rdfs:comment "Strategy used: 'forward-chain', 'backward-chain', 'hybrid'."@en ;
    rdfs:domain rsn:Inference ;
    rdfs:range xsd:string .

# =============================================================================
# SLO / QUALITY VOCABULARY
# Provides comprehensive Service Level Objectives and quality metrics
# for capabilities, protocols, and system components
# =============================================================================

# -----------------------------------------------------------------------------
# Core SLO Classes
# -----------------------------------------------------------------------------

slo:ServiceLevelObjective a rdfs:Class, owl:Class ;
    rdfs:label "Service Level Objective"@en ;
    rdfs:comment "A measurable target for service quality."@en .

slo:PerformanceSLO a rdfs:Class, owl:Class ;
    rdfs:label "Performance SLO"@en ;
    rdfs:comment "SLO related to performance metrics (latency, throughput)."@en ;
    rdfs:subClassOf slo:ServiceLevelObjective .

slo:AvailabilitySLO a rdfs:Class, owl:Class ;
    rdfs:label "Availability SLO"@en ;
    rdfs:comment "SLO related to system availability."@en ;
    rdfs:subClassOf slo:ServiceLevelObjective .

slo:QualitySLO a rdfs:Class, owl:Class ;
    rdfs:label "Quality SLO"@en ;
    rdfs:comment "SLO related to output quality metrics."@en ;
    rdfs:subClassOf slo:ServiceLevelObjective .

slo:CostSLO a rdfs:Class, owl:Class ;
    rdfs:label "Cost SLO"@en ;
    rdfs:comment "SLO related to resource costs."@en ;
    rdfs:subClassOf slo:ServiceLevelObjective .

slo:Metric a rdfs:Class, owl:Class ;
    rdfs:label "Metric"@en ;
    rdfs:comment "A measurable quantity."@en .

slo:Threshold a rdfs:Class, owl:Class ;
    rdfs:label "Threshold"@en ;
    rdfs:comment "A threshold value for SLO evaluation."@en .

slo:Measurement a rdfs:Class, owl:Class ;
    rdfs:label "Measurement"@en ;
    rdfs:comment "An actual measurement of a metric."@en .

slo:Alert a rdfs:Class, owl:Class ;
    rdfs:label "Alert"@en ;
    rdfs:comment "An alert triggered by SLO violation."@en .

slo:Budget a rdfs:Class, owl:Class ;
    rdfs:label "Error Budget"@en ;
