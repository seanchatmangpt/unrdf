# UNRDF Documentation Ontology
# Comprehensive RDF ontology for autonomic documentation enabling:
# - Capability Discovery
# - Protocol Navigation
# - Type Understanding
# - Reasoning Support
# - SLO/Quality Metrics
#
# Base namespace: urn:unrdf:doc:
# Compatible with: N3 reasoning, SPARQL querying, SHACL validation
# Version: 1.0.0

@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix prov: <http://www.w3.org/ns/prov#> .

# Base namespace for UNRDF Documentation Ontology
@prefix doc: <urn:unrdf:doc:> .

# Sub-namespaces for vocabulary organization
@prefix cap: <urn:unrdf:doc:capability:> .
@prefix proto: <urn:unrdf:doc:protocol:> .
@prefix typ: <urn:unrdf:doc:type:> .
@prefix rsn: <urn:unrdf:doc:reasoning:> .
@prefix slo: <urn:unrdf:doc:slo:> .
@prefix auth: <urn:unrdf:doc:auth:> .
@prefix cost: <urn:unrdf:doc:cost:> .
@prefix state: <urn:unrdf:doc:state:> .

# =============================================================================
# ONTOLOGY METADATA
# =============================================================================

doc:DocumentationOntology a owl:Ontology ;
    rdfs:label "UNRDF Documentation Ontology"@en ;
    rdfs:comment "Comprehensive RDF ontology for autonomic documentation enabling capability discovery, protocol navigation, type understanding, and reasoning support for autonomous agents."@en ;
    owl:versionInfo "1.0.0" ;
    dcterms:created "2024-01-01"^^xsd:date ;
    dcterms:creator "UNRDF Project" ;
    dcterms:license <https://opensource.org/licenses/MIT> .

# =============================================================================
# CAPABILITY VOCABULARY
# Enables agents to discover and match capabilities by preconditions,
# postconditions, performance requirements, cost constraints, and auth levels
# =============================================================================

# -----------------------------------------------------------------------------
# Core Capability Classes
# -----------------------------------------------------------------------------

cap:Capability a rdfs:Class, owl:Class ;
    rdfs:label "Capability"@en ;
    rdfs:comment "A discrete unit of functionality that an agent or system can perform. Capabilities are discoverable via preconditions/postconditions and constrained by performance, cost, and authorization requirements."@en ;
    rdfs:subClassOf prov:Activity .

cap:AtomicCapability a rdfs:Class, owl:Class ;
    rdfs:label "Atomic Capability"@en ;
    rdfs:comment "A capability that cannot be decomposed further - the smallest unit of executable functionality."@en ;
    rdfs:subClassOf cap:Capability .

cap:CompositeCapability a rdfs:Class, owl:Class ;
    rdfs:label "Composite Capability"@en ;
    rdfs:comment "A capability composed of multiple sub-capabilities executed in sequence or parallel."@en ;
    rdfs:subClassOf cap:Capability .

cap:Condition a rdfs:Class, owl:Class ;
    rdfs:label "Condition"@en ;
    rdfs:comment "A logical condition that must be satisfied. Used for preconditions, postconditions, and invariants."@en .

cap:Precondition a rdfs:Class, owl:Class ;
    rdfs:label "Precondition"@en ;
    rdfs:comment "A condition that must be true before a capability can be executed."@en ;
    rdfs:subClassOf cap:Condition .

cap:Postcondition a rdfs:Class, owl:Class ;
    rdfs:label "Postcondition"@en ;
    rdfs:comment "A condition that will be true after successful capability execution."@en ;
    rdfs:subClassOf cap:Condition .

cap:Invariant a rdfs:Class, owl:Class ;
    rdfs:label "Invariant"@en ;
    rdfs:comment "A condition that must remain true throughout capability execution."@en ;
    rdfs:subClassOf cap:Condition .

cap:Effect a rdfs:Class, owl:Class ;
    rdfs:label "Effect"@en ;
    rdfs:comment "A side effect or state change resulting from capability execution."@en .

cap:Resource a rdfs:Class, owl:Class ;
    rdfs:label "Resource"@en ;
    rdfs:comment "A resource consumed, produced, or modified by a capability."@en .

# -----------------------------------------------------------------------------
# Capability Properties
# -----------------------------------------------------------------------------

cap:hasPrecondition a rdf:Property, owl:ObjectProperty ;
    rdfs:label "has precondition"@en ;
    rdfs:comment "Links a capability to a condition that must be satisfied before execution."@en ;
    rdfs:domain cap:Capability ;
    rdfs:range cap:Precondition .

cap:hasPostcondition a rdf:Property, owl:ObjectProperty ;
    rdfs:label "has postcondition"@en ;
    rdfs:comment "Links a capability to a condition guaranteed after successful execution."@en ;
    rdfs:domain cap:Capability ;
    rdfs:range cap:Postcondition .

cap:hasInvariant a rdf:Property, owl:ObjectProperty ;
    rdfs:label "has invariant"@en ;
    rdfs:comment "Links a capability to a condition that must hold throughout execution."@en ;
    rdfs:domain cap:Capability ;
    rdfs:range cap:Invariant .

cap:hasEffect a rdf:Property, owl:ObjectProperty ;
    rdfs:label "has effect"@en ;
    rdfs:comment "Links a capability to a side effect it produces."@en ;
    rdfs:domain cap:Capability ;
    rdfs:range cap:Effect .

cap:requires a rdf:Property, owl:ObjectProperty ;
    rdfs:label "requires"@en ;
    rdfs:comment "Links a capability to a resource it requires for execution."@en ;
    rdfs:domain cap:Capability ;
    rdfs:range cap:Resource .

cap:produces a rdf:Property, owl:ObjectProperty ;
    rdfs:label "produces"@en ;
    rdfs:comment "Links a capability to a resource it produces."@en ;
    rdfs:domain cap:Capability ;
    rdfs:range cap:Resource .

cap:consumes a rdf:Property, owl:ObjectProperty ;
    rdfs:label "consumes"@en ;
    rdfs:comment "Links a capability to a resource it consumes (depletes)."@en ;
    rdfs:domain cap:Capability ;
    rdfs:range cap:Resource .

cap:modifies a rdf:Property, owl:ObjectProperty ;
    rdfs:label "modifies"@en ;
    rdfs:comment "Links a capability to a resource it modifies in place."@en ;
    rdfs:domain cap:Capability ;
    rdfs:range cap:Resource .

cap:composedOf a rdf:Property, owl:ObjectProperty ;
    rdfs:label "composed of"@en ;
    rdfs:comment "Links a composite capability to its constituent sub-capabilities."@en ;
    rdfs:domain cap:CompositeCapability ;
    rdfs:range cap:Capability .

cap:executionOrder a rdf:Property, owl:DatatypeProperty ;
    rdfs:label "execution order"@en ;
    rdfs:comment "Specifies the execution order of sub-capabilities: 'sequential', 'parallel', or 'conditional'."@en ;
    rdfs:domain cap:CompositeCapability ;
    rdfs:range xsd:string .

cap:conditionExpression a rdf:Property, owl:DatatypeProperty ;
    rdfs:label "condition expression"@en ;
    rdfs:comment "A SPARQL ASK query or N3 formula expressing the condition logic."@en ;
    rdfs:domain cap:Condition ;
    rdfs:range xsd:string .

cap:conditionSparql a rdf:Property, owl:DatatypeProperty ;
    rdfs:label "condition SPARQL"@en ;
    rdfs:comment "A SPARQL ASK query that evaluates the condition against the knowledge graph."@en ;
    rdfs:domain cap:Condition ;
    rdfs:range xsd:string .

cap:conditionN3 a rdf:Property, owl:DatatypeProperty ;
    rdfs:label "condition N3"@en ;
    rdfs:comment "An N3 formula for logical reasoning about the condition."@en ;
    rdfs:domain cap:Condition ;
    rdfs:range xsd:string .

cap:inputType a rdf:Property, owl:ObjectProperty ;
    rdfs:label "input type"@en ;
    rdfs:comment "Links a capability to its expected input type."@en ;
    rdfs:domain cap:Capability ;
    rdfs:range rdfs:Class .

cap:outputType a rdf:Property, owl:ObjectProperty ;
    rdfs:label "output type"@en ;
    rdfs:comment "Links a capability to its output type."@en ;
    rdfs:domain cap:Capability ;
    rdfs:range rdfs:Class .

cap:implementedBy a rdf:Property, owl:ObjectProperty ;
    rdfs:label "implemented by"@en ;
    rdfs:comment "Links a capability to the agent or service that implements it."@en ;
    rdfs:domain cap:Capability ;
    rdfs:range prov:Agent .

cap:version a rdf:Property, owl:DatatypeProperty ;
    rdfs:label "version"@en ;
    rdfs:comment "Semantic version of the capability."@en ;
    rdfs:domain cap:Capability ;
    rdfs:range xsd:string .

cap:deprecated a rdf:Property, owl:DatatypeProperty ;
    rdfs:label "deprecated"@en ;
    rdfs:comment "Indicates if the capability is deprecated."@en ;
    rdfs:domain cap:Capability ;
    rdfs:range xsd:boolean .

cap:supersededBy a rdf:Property, owl:ObjectProperty ;
    rdfs:label "superseded by"@en ;
    rdfs:comment "Links a deprecated capability to its replacement."@en ;
    rdfs:domain cap:Capability ;
    rdfs:range cap:Capability .

# =============================================================================
# PROTOCOL VOCABULARY
# Enables agents to understand valid state transitions, message formats,
# error handling, consensus requirements, and timeout strategies
# =============================================================================

# -----------------------------------------------------------------------------
# Core Protocol Classes
# -----------------------------------------------------------------------------

proto:Protocol a rdfs:Class, owl:Class ;
    rdfs:label "Protocol"@en ;
    rdfs:comment "A formal specification of interaction patterns between agents or components, including state machines, message formats, and error handling."@en .

proto:State a rdfs:Class, owl:Class ;
    rdfs:label "State"@en ;
    rdfs:comment "A discrete state within a protocol's state machine."@en .

proto:InitialState a rdfs:Class, owl:Class ;
    rdfs:label "Initial State"@en ;
    rdfs:comment "The starting state of a protocol."@en ;
    rdfs:subClassOf proto:State .

proto:FinalState a rdfs:Class, owl:Class ;
    rdfs:label "Final State"@en ;
    rdfs:comment "A terminal state of a protocol (success or failure)."@en ;
    rdfs:subClassOf proto:State .

proto:ErrorState a rdfs:Class, owl:Class ;
    rdfs:label "Error State"@en ;
    rdfs:comment "A state representing an error condition."@en ;
    rdfs:subClassOf proto:FinalState .

proto:Transition a rdfs:Class, owl:Class ;
    rdfs:label "Transition"@en ;
    rdfs:comment "A valid state transition within a protocol, triggered by events or conditions."@en .

proto:Message a rdfs:Class, owl:Class ;
    rdfs:label "Message"@en ;
    rdfs:comment "A message type used in protocol communication."@en .

proto:Request a rdfs:Class, owl:Class ;
    rdfs:label "Request"@en ;
    rdfs:comment "A request message initiating an interaction."@en ;
    rdfs:subClassOf proto:Message .

proto:Response a rdfs:Class, owl:Class ;
    rdfs:label "Response"@en ;
    rdfs:comment "A response message replying to a request."@en ;
    rdfs:subClassOf proto:Message .

proto:Event a rdfs:Class, owl:Class ;
    rdfs:label "Event"@en ;
    rdfs:comment "An event that can trigger state transitions."@en ;
    rdfs:subClassOf proto:Message .

proto:ErrorHandler a rdfs:Class, owl:Class ;
    rdfs:label "Error Handler"@en ;
    rdfs:comment "A handler for protocol errors with recovery strategies."@en .

proto:TimeoutStrategy a rdfs:Class, owl:Class ;
    rdfs:label "Timeout Strategy"@en ;
    rdfs:comment "A strategy for handling timeouts in protocol execution."@en .

proto:ConsensusRequirement a rdfs:Class, owl:Class ;
    rdfs:label "Consensus Requirement"@en ;
    rdfs:comment "Requirements for achieving consensus in distributed protocols."@en .

proto:RetryPolicy a rdfs:Class, owl:Class ;
    rdfs:label "Retry Policy"@en ;
    rdfs:comment "A policy defining retry behavior for failed operations."@en .

# -----------------------------------------------------------------------------
# Protocol Properties
# -----------------------------------------------------------------------------

proto:hasState a rdf:Property, owl:ObjectProperty ;
    rdfs:label "has state"@en ;
    rdfs:comment "Links a protocol to one of its states."@en ;
    rdfs:domain proto:Protocol ;
    rdfs:range proto:State .

proto:hasInitialState a rdf:Property, owl:ObjectProperty ;
    rdfs:label "has initial state"@en ;
    rdfs:comment "Links a protocol to its initial state."@en ;
    rdfs:domain proto:Protocol ;
    rdfs:range proto:InitialState ;
    rdfs:subPropertyOf proto:hasState .

proto:hasFinalState a rdf:Property, owl:ObjectProperty ;
    rdfs:label "has final state"@en ;
    rdfs:comment "Links a protocol to a final state."@en ;
    rdfs:domain proto:Protocol ;
    rdfs:range proto:FinalState ;
    rdfs:subPropertyOf proto:hasState .

proto:hasTransition a rdf:Property, owl:ObjectProperty ;
    rdfs:label "has transition"@en ;
    rdfs:comment "Links a protocol to a state transition."@en ;
    rdfs:domain proto:Protocol ;
    rdfs:range proto:Transition .

proto:fromState a rdf:Property, owl:ObjectProperty ;
    rdfs:label "from state"@en ;
    rdfs:comment "The source state of a transition."@en ;
    rdfs:domain proto:Transition ;
    rdfs:range proto:State .

proto:toState a rdf:Property, owl:ObjectProperty ;
    rdfs:label "to state"@en ;
    rdfs:comment "The target state of a transition."@en ;
    rdfs:domain proto:Transition ;
    rdfs:range proto:State .

proto:triggeredBy a rdf:Property, owl:ObjectProperty ;
    rdfs:label "triggered by"@en ;
    rdfs:comment "The event or message that triggers a transition."@en ;
    rdfs:domain proto:Transition ;
    rdfs:range proto:Message .

proto:guard a rdf:Property, owl:DatatypeProperty ;
    rdfs:label "guard"@en ;
    rdfs:comment "A SPARQL ASK condition that must be true for the transition to occur."@en ;
    rdfs:domain proto:Transition ;
    rdfs:range xsd:string .

proto:action a rdf:Property, owl:ObjectProperty ;
    rdfs:label "action"@en ;
    rdfs:comment "A capability to execute during the transition."@en ;
    rdfs:domain proto:Transition ;
    rdfs:range cap:Capability .

proto:hasMessage a rdf:Property, owl:ObjectProperty ;
    rdfs:label "has message"@en ;
    rdfs:comment "Links a protocol to a message type it uses."@en ;
    rdfs:domain proto:Protocol ;
    rdfs:range proto:Message .

proto:messageSchema a rdf:Property, owl:ObjectProperty ;
    rdfs:label "message schema"@en ;
    rdfs:comment "Links a message to its SHACL shape for validation."@en ;
    rdfs:domain proto:Message ;
    rdfs:range sh:NodeShape .

proto:hasErrorHandler a rdf:Property, owl:ObjectProperty ;
    rdfs:label "has error handler"@en ;
    rdfs:comment "Links a protocol to an error handler."@en ;
    rdfs:domain proto:Protocol ;
    rdfs:range proto:ErrorHandler .

proto:handlesError a rdf:Property, owl:DatatypeProperty ;
    rdfs:label "handles error"@en ;
    rdfs:comment "The error type handled by this handler."@en ;
    rdfs:domain proto:ErrorHandler ;
    rdfs:range xsd:string .

proto:recoveryAction a rdf:Property, owl:ObjectProperty ;
    rdfs:label "recovery action"@en ;
    rdfs:comment "The capability to execute for error recovery."@en ;
    rdfs:domain proto:ErrorHandler ;
    rdfs:range cap:Capability .

proto:fallbackState a rdf:Property, owl:ObjectProperty ;
    rdfs:label "fallback state"@en ;
    rdfs:comment "The state to transition to on error."@en ;
    rdfs:domain proto:ErrorHandler ;
    rdfs:range proto:State .

proto:hasTimeoutStrategy a rdf:Property, owl:ObjectProperty ;
    rdfs:label "has timeout strategy"@en ;
    rdfs:comment "Links a protocol to its timeout strategy."@en ;
    rdfs:domain proto:Protocol ;
    rdfs:range proto:TimeoutStrategy .

proto:timeoutMs a rdf:Property, owl:DatatypeProperty ;
    rdfs:label "timeout milliseconds"@en ;
    rdfs:comment "Timeout duration in milliseconds."@en ;
    rdfs:domain proto:TimeoutStrategy ;
    rdfs:range xsd:integer .

proto:timeoutAction a rdf:Property, owl:DatatypeProperty ;
    rdfs:label "timeout action"@en ;
    rdfs:comment "Action to take on timeout: 'retry', 'abort', 'escalate'."@en ;
    rdfs:domain proto:TimeoutStrategy ;
    rdfs:range xsd:string .

proto:hasRetryPolicy a rdf:Property, owl:ObjectProperty ;
    rdfs:label "has retry policy"@en ;
    rdfs:comment "Links a protocol to its retry policy."@en ;
    rdfs:domain proto:Protocol ;
    rdfs:range proto:RetryPolicy .

proto:maxRetries a rdf:Property, owl:DatatypeProperty ;
    rdfs:label "max retries"@en ;
    rdfs:comment "Maximum number of retry attempts."@en ;
    rdfs:domain proto:RetryPolicy ;
    rdfs:range xsd:integer .

proto:retryBackoff a rdf:Property, owl:DatatypeProperty ;
    rdfs:label "retry backoff"@en ;
    rdfs:comment "Backoff strategy: 'linear', 'exponential', 'fibonacci'."@en ;
    rdfs:domain proto:RetryPolicy ;
    rdfs:range xsd:string .

proto:retryDelayMs a rdf:Property, owl:DatatypeProperty ;
    rdfs:label "retry delay milliseconds"@en ;
    rdfs:comment "Base delay between retries in milliseconds."@en ;
    rdfs:domain proto:RetryPolicy ;
    rdfs:range xsd:integer .

proto:hasConsensusRequirement a rdf:Property, owl:ObjectProperty ;
    rdfs:label "has consensus requirement"@en ;
    rdfs:comment "Links a protocol to its consensus requirements."@en ;
    rdfs:domain proto:Protocol ;
    rdfs:range proto:ConsensusRequirement .

proto:quorum a rdf:Property, owl:DatatypeProperty ;
    rdfs:label "quorum"@en ;
    rdfs:comment "Minimum fraction of participants required for consensus (0-1)."@en ;
    rdfs:domain proto:ConsensusRequirement ;
    rdfs:range xsd:decimal .

proto:consensusAlgorithm a rdf:Property, owl:DatatypeProperty ;
    rdfs:label "consensus algorithm"@en ;
    rdfs:comment "The consensus algorithm: 'voting', 'raft', 'pbft', 'crdt'."@en ;
    rdfs:domain proto:ConsensusRequirement ;
    rdfs:range xsd:string .

proto:byzantineTolerance a rdf:Property, owl:DatatypeProperty ;
    rdfs:label "byzantine tolerance"@en ;
    rdfs:comment "Maximum fraction of Byzantine (faulty) nodes tolerated."@en ;
    rdfs:domain proto:ConsensusRequirement ;
    rdfs:range xsd:decimal .

# =============================================================================
# TYPE VOCABULARY
# Provides full semantic type system via RDF including class hierarchies,
# property domains/ranges, constraints, invariants, and equivalence
# =============================================================================

# -----------------------------------------------------------------------------
# Core Type Classes
# -----------------------------------------------------------------------------

typ:Type a rdfs:Class, owl:Class ;
    rdfs:label "Type"@en ;
    rdfs:comment "A semantic type in the UNRDF type system."@en .

typ:PrimitiveType a rdfs:Class, owl:Class ;
    rdfs:label "Primitive Type"@en ;
    rdfs:comment "A primitive/scalar type (string, integer, boolean, etc.)."@en ;
    rdfs:subClassOf typ:Type .

typ:ComplexType a rdfs:Class, owl:Class ;
    rdfs:label "Complex Type"@en ;
    rdfs:comment "A complex type with properties and structure."@en ;
    rdfs:subClassOf typ:Type .

typ:CollectionType a rdfs:Class, owl:Class ;
    rdfs:label "Collection Type"@en ;
    rdfs:comment "A type representing a collection (list, set, map)."@en ;
    rdfs:subClassOf typ:Type .

typ:UnionType a rdfs:Class, owl:Class ;
    rdfs:label "Union Type"@en ;
    rdfs:comment "A type that can be one of several types."@en ;
    rdfs:subClassOf typ:Type .

typ:IntersectionType a rdfs:Class, owl:Class ;
    rdfs:label "Intersection Type"@en ;
    rdfs:comment "A type combining multiple types."@en ;
    rdfs:subClassOf typ:Type .

typ:GenericType a rdfs:Class, owl:Class ;
    rdfs:label "Generic Type"@en ;
    rdfs:comment "A parameterized generic type."@en ;
    rdfs:subClassOf typ:Type .

typ:TypeConstraint a rdfs:Class, owl:Class ;
    rdfs:label "Type Constraint"@en ;
    rdfs:comment "A constraint on type values."@en .

typ:TypeParameter a rdfs:Class, owl:Class ;
    rdfs:label "Type Parameter"@en ;
    rdfs:comment "A parameter for generic types."@en .

typ:Property a rdfs:Class, owl:Class ;
    rdfs:label "Property"@en ;
    rdfs:comment "A property definition within a complex type."@en .

# -----------------------------------------------------------------------------
# Type Properties
# -----------------------------------------------------------------------------

typ:extendsType a rdf:Property, owl:ObjectProperty ;
    rdfs:label "extends type"@en ;
    rdfs:comment "Links a type to its parent type in the hierarchy."@en ;
    rdfs:domain typ:Type ;
    rdfs:range typ:Type .

typ:implementsType a rdf:Property, owl:ObjectProperty ;
    rdfs:label "implements type"@en ;
    rdfs:comment "Links a type to an interface/trait it implements."@en ;
    rdfs:domain typ:Type ;
    rdfs:range typ:Type .

typ:hasProperty a rdf:Property, owl:ObjectProperty ;
    rdfs:label "has property"@en ;
    rdfs:comment "Links a complex type to its properties."@en ;
    rdfs:domain typ:ComplexType ;
    rdfs:range typ:Property .

typ:propertyType a rdf:Property, owl:ObjectProperty ;
    rdfs:label "property type"@en ;
    rdfs:comment "The type of a property."@en ;
    rdfs:domain typ:Property ;
    rdfs:range typ:Type .

typ:propertyName a rdf:Property, owl:DatatypeProperty ;
    rdfs:label "property name"@en ;
    rdfs:comment "The name of a property."@en ;
    rdfs:domain typ:Property ;
    rdfs:range xsd:string .

typ:required a rdf:Property, owl:DatatypeProperty ;
    rdfs:label "required"@en ;
    rdfs:comment "Whether a property is required."@en ;
    rdfs:domain typ:Property ;
    rdfs:range xsd:boolean .

typ:defaultValue a rdf:Property, owl:DatatypeProperty ;
    rdfs:label "default value"@en ;
    rdfs:comment "The default value for a property."@en ;
    rdfs:domain typ:Property ;
    rdfs:range rdfs:Literal .

typ:hasConstraint a rdf:Property, owl:ObjectProperty ;
    rdfs:label "has constraint"@en ;
    rdfs:comment "Links a type or property to a constraint."@en ;
    rdfs:domain [ owl:unionOf ( typ:Type typ:Property ) ] ;
    rdfs:range typ:TypeConstraint .

typ:constraintExpression a rdf:Property, owl:DatatypeProperty ;
    rdfs:label "constraint expression"@en ;
    rdfs:comment "A SHACL or N3 expression defining the constraint."@en ;
    rdfs:domain typ:TypeConstraint ;
    rdfs:range xsd:string .

typ:minValue a rdf:Property, owl:DatatypeProperty ;
    rdfs:label "minimum value"@en ;
    rdfs:comment "Minimum allowed value."@en ;
    rdfs:domain typ:TypeConstraint ;
    rdfs:range rdfs:Literal .

typ:maxValue a rdf:Property, owl:DatatypeProperty ;
    rdfs:label "maximum value"@en ;
    rdfs:comment "Maximum allowed value."@en ;
    rdfs:domain typ:TypeConstraint ;
    rdfs:range rdfs:Literal .

typ:pattern a rdf:Property, owl:DatatypeProperty ;
    rdfs:label "pattern"@en ;
    rdfs:comment "Regex pattern for string validation."@en ;
    rdfs:domain typ:TypeConstraint ;
    rdfs:range xsd:string .

typ:enumValues a rdf:Property, owl:DatatypeProperty ;
    rdfs:label "enum values"@en ;
    rdfs:comment "Allowed enumeration values (comma-separated)."@en ;
    rdfs:domain typ:TypeConstraint ;
    rdfs:range xsd:string .

typ:unionOf a rdf:Property, owl:ObjectProperty ;
    rdfs:label "union of"@en ;
    rdfs:comment "Links a union type to its constituent types."@en ;
    rdfs:domain typ:UnionType ;
    rdfs:range rdf:List .

typ:intersectionOf a rdf:Property, owl:ObjectProperty ;
    rdfs:label "intersection of"@en ;
    rdfs:comment "Links an intersection type to its constituent types."@en ;
    rdfs:domain typ:IntersectionType ;
    rdfs:range rdf:List .

typ:elementType a rdf:Property, owl:ObjectProperty ;
    rdfs:label "element type"@en ;
    rdfs:comment "The element type of a collection."@en ;
    rdfs:domain typ:CollectionType ;
    rdfs:range typ:Type .

typ:collectionKind a rdf:Property, owl:DatatypeProperty ;
    rdfs:label "collection kind"@en ;
    rdfs:comment "The kind of collection: 'list', 'set', 'map', 'array'."@en ;
    rdfs:domain typ:CollectionType ;
    rdfs:range xsd:string .

typ:keyType a rdf:Property, owl:ObjectProperty ;
    rdfs:label "key type"@en ;
    rdfs:comment "The key type for map collections."@en ;
    rdfs:domain typ:CollectionType ;
    rdfs:range typ:Type .

typ:hasParameter a rdf:Property, owl:ObjectProperty ;
    rdfs:label "has parameter"@en ;
    rdfs:comment "Links a generic type to its type parameters."@en ;
    rdfs:domain typ:GenericType ;
    rdfs:range typ:TypeParameter .

typ:parameterBound a rdf:Property, owl:ObjectProperty ;
    rdfs:label "parameter bound"@en ;
    rdfs:comment "Upper bound constraint for a type parameter."@en ;
    rdfs:domain typ:TypeParameter ;
    rdfs:range typ:Type .

typ:equivalentTo a rdf:Property, owl:ObjectProperty ;
    rdfs:label "equivalent to"@en ;
    rdfs:comment "Declares type equivalence."@en ;
    rdfs:domain typ:Type ;
    rdfs:range typ:Type ;
    a owl:SymmetricProperty, owl:TransitiveProperty .

typ:subtypeOf a rdf:Property, owl:ObjectProperty ;
    rdfs:label "subtype of"@en ;
    rdfs:comment "Declares subtype relationship."@en ;
    rdfs:domain typ:Type ;
    rdfs:range typ:Type ;
    a owl:TransitiveProperty .

typ:shaclShape a rdf:Property, owl:ObjectProperty ;
    rdfs:label "SHACL shape"@en ;
    rdfs:comment "Links a type to its SHACL validation shape."@en ;
    rdfs:domain typ:Type ;
    rdfs:range sh:NodeShape .

typ:zodSchema a rdf:Property, owl:DatatypeProperty ;
    rdfs:label "Zod schema"@en ;
    rdfs:comment "A Zod schema string for runtime validation."@en ;
    rdfs:domain typ:Type ;
