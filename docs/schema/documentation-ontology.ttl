    rdfs:comment "The percentile for latency SLOs (e.g., p50, p95, p99)."@en ;
    rdfs:domain slo:PerformanceSLO ;
    rdfs:range xsd:integer .

slo:latencyMs a rdf:Property, owl:DatatypeProperty ;
    rdfs:label "latency milliseconds"@en ;
    rdfs:comment "Target latency in milliseconds."@en ;
    rdfs:domain slo:PerformanceSLO ;
    rdfs:range xsd:integer .

slo:throughput a rdf:Property, owl:DatatypeProperty ;
    rdfs:label "throughput"@en ;
    rdfs:comment "Target throughput (operations per second)."@en ;
    rdfs:domain slo:PerformanceSLO ;
    rdfs:range xsd:decimal .

slo:uptimePercent a rdf:Property, owl:DatatypeProperty ;
    rdfs:label "uptime percent"@en ;
    rdfs:comment "Target uptime percentage (e.g., 99.9)."@en ;
    rdfs:domain slo:AvailabilitySLO ;
    rdfs:range xsd:decimal .

slo:mttr a rdf:Property, owl:DatatypeProperty ;
    rdfs:label "mean time to recovery"@en ;
    rdfs:comment "Target MTTR in minutes."@en ;
    rdfs:domain slo:AvailabilitySLO ;
    rdfs:range xsd:decimal .

slo:mtbf a rdf:Property, owl:DatatypeProperty ;
    rdfs:label "mean time between failures"@en ;
    rdfs:comment "Target MTBF in hours."@en ;
    rdfs:domain slo:AvailabilitySLO ;
    rdfs:range xsd:decimal .

slo:errorRate a rdf:Property, owl:DatatypeProperty ;
    rdfs:label "error rate"@en ;
    rdfs:comment "Maximum acceptable error rate (0-1)."@en ;
    rdfs:domain slo:QualitySLO ;
    rdfs:range xsd:decimal .

slo:accuracy a rdf:Property, owl:DatatypeProperty ;
    rdfs:label "accuracy"@en ;
    rdfs:comment "Target accuracy (0-1)."@en ;
    rdfs:domain slo:QualitySLO ;
    rdfs:range xsd:decimal .

slo:maxCost a rdf:Property, owl:DatatypeProperty ;
    rdfs:label "maximum cost"@en ;
    rdfs:comment "Maximum cost per operation."@en ;
    rdfs:domain slo:CostSLO ;
    rdfs:range xsd:decimal .

slo:costUnit a rdf:Property, owl:DatatypeProperty ;
    rdfs:label "cost unit"@en ;
    rdfs:comment "Unit of cost measurement (tokens, credits, USD)."@en ;
    rdfs:domain slo:CostSLO ;
    rdfs:range xsd:string .

slo:hasMeasurement a rdf:Property, owl:ObjectProperty ;
    rdfs:label "has measurement"@en ;
    rdfs:comment "Links an SLO to actual measurements."@en ;
    rdfs:domain slo:ServiceLevelObjective ;
    rdfs:range slo:Measurement .

slo:measurementValue a rdf:Property, owl:DatatypeProperty ;
    rdfs:label "measurement value"@en ;
    rdfs:comment "The actual measured value."@en ;
    rdfs:domain slo:Measurement ;
    rdfs:range xsd:decimal .

slo:measurementTimestamp a rdf:Property, owl:DatatypeProperty ;
    rdfs:label "measurement timestamp"@en ;
    rdfs:comment "When the measurement was taken."@en ;
    rdfs:domain slo:Measurement ;
    rdfs:range xsd:dateTime .

slo:meetsTarget a rdf:Property, owl:DatatypeProperty ;
    rdfs:label "meets target"@en ;
    rdfs:comment "Whether the measurement meets the SLO target."@en ;
    rdfs:domain slo:Measurement ;
    rdfs:range xsd:boolean .

slo:hasAlert a rdf:Property, owl:ObjectProperty ;
    rdfs:label "has alert"@en ;
    rdfs:comment "Links an SLO to its alert configuration."@en ;
    rdfs:domain slo:ServiceLevelObjective ;
    rdfs:range slo:Alert .

slo:alertSeverity a rdf:Property, owl:DatatypeProperty ;
    rdfs:label "alert severity"@en ;
    rdfs:comment "Severity level: 'info', 'warning', 'critical'."@en ;
    rdfs:domain slo:Alert ;
    rdfs:range xsd:string .

slo:alertThreshold a rdf:Property, owl:DatatypeProperty ;
    rdfs:label "alert threshold"@en ;
    rdfs:comment "Percentage of budget consumed to trigger alert."@en ;
    rdfs:domain slo:Alert ;
    rdfs:range xsd:decimal .

slo:hasErrorBudget a rdf:Property, owl:ObjectProperty ;
    rdfs:label "has error budget"@en ;
    rdfs:comment "Links an SLO to its error budget."@en ;
    rdfs:domain slo:ServiceLevelObjective ;
    rdfs:range slo:Budget .

slo:budgetPeriod a rdf:Property, owl:DatatypeProperty ;
    rdfs:label "budget period"@en ;
    rdfs:comment "Time period for budget calculation (e.g., 'monthly')."@en ;
    rdfs:domain slo:Budget ;
    rdfs:range xsd:string .

slo:budgetRemaining a rdf:Property, owl:DatatypeProperty ;
    rdfs:label "budget remaining"@en ;
    rdfs:comment "Remaining error budget (0-1)."@en ;
    rdfs:domain slo:Budget ;
    rdfs:range xsd:decimal .

